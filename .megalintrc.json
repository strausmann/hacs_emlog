{
  "FLAVOR": "python",
  "PYTHON_PYLINT_DISABLED": false,
  "PYTHON_MYPY_DISABLED": false,
  "PYTHON_PYRIGHT_DISABLED": false,
  "PYTHON_BLACK_DISABLED": false,
  "PYTHON_ISORT_DISABLED": false,
  "PYTHON_FLAKE8_DISABLED": false,
  "YAML_YAMLLINT_DISABLED": false,
  "JSON_JSONLINT_DISABLED": false,
  "MARKDOWN_MARKDOWNLINT_DISABLED": false,
  "DOCKERFILE_HADOLINT_DISABLED": false,
  "BASH_SHELLCHECK_DISABLED": false,
  "COPYPASTE_JSCPD_DISABLED": false,
  "REPOSITORY_DEVSKIM_DISABLED": false,
  "REPOSITORY_DEVSKIM_FILTER_REGEX_INCLUDE": "^DS137138$",
  "SPELL_CSPELL_DISABLED": false,
  "SPELL_CSPELL_FILTER_REGEX_EXCLUDE": "(CHANGELOG|node_modules|venv|__pycache__|megalinter-reports)",
  "EDITORCONFIG_EDITORCONFIG_CHECKER_DISABLED": true,
  "YAML_V8R_DISABLED": true,
  "GITHUB_WORKSPACE": "/tmp/lint",
  "ENABLE_LINTERS": [
    "PYTHON_BLACK",
    "PYTHON_ISORT",
    "PYTHON_FLAKE8",
    "YAML_YAMLLINT",
    "JSON_JSONLINT",
    "MARKDOWN_MARKDOWNLINT",
    "DOCKERFILE_HADOLINT",
    "BASH_SHELLCHECK",
    "COPYPASTE_JSCPD",
    "REPOSITORY_DEVSKIM",
    "SPELL_CSPELL",
    "SPELL_LYCHEE"
  ],
  "PYTHON_FLAKE8_ARGUMENTS": "--max-line-length=120 --extend-ignore=E203,W503",
  "PYTHON_BLACK_ARGUMENTS": "--line-length=120",
  "PYTHON_ISORT_ARGUMENTS": "--profile=black --line-length=120",
  "MARKDOWN_MARKDOWNLINT_DISABLE_ERRORS": "MD013,MD024",
  "COPYPASTE_JSCPD_THRESHOLD": 6,
  "COPYPASTE_JSCPD_EXCLUDE_PATHS": "custom_components/emlog/sensor.py,custom_components/emlog/template.py",
  "FAIL_IF_UPDATED_SOURCES": false
}
